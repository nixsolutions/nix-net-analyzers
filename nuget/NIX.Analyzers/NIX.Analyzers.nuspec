<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>NIX.Analyzers</id>
    <version>$version$</version>
    <authors>NIX .NET Department</authors>
    <licenseUrl>https://github.com/nixsolutions/nix-net-analyzers/blob/main/LICENSE</licenseUrl>
    <projectUrl>https://github.com/nixsolutions/nix-net-analyzers</projectUrl>
    <iconUrl>https://www.nixsolutions.com/wp-content/themes/nixsite6g/favicons/NIX_64x64.png</iconUrl>
    <description>Provides analyzers for validating code with nix custom rules</description>
    <copyright>$copyright$</copyright>
    <tags>Analyzers, Roslyn</tags>
    <repository url="https://github.com/nixsolutions/nix-net-analyzers" />
    <dependencies>
      <dependency id="StyleCop.Analyzers" version="1.1.118" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System" targetFramework="" />
    </frameworkAssemblies>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <files>
    <file src=".\bin\Debug\netstandard2.0\*.dll"  target="analyzers\dotnet\cs" />
    <file src="assets\tools\*.ps1" target="tools\" />
    <file src="assets\config\.editorconfig"  target="content\Rules" />
    <file src="assets\docs\readme.txt"  target="" />
  </files>
</package>
